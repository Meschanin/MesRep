<HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.sberbank.kka.view.ServiceViewController">
   <children>
      <Label fx:id="svcNameLabel" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="25.0" prefWidth="200.0" text="ServiceName">
